#NERDTree


#Plugins
<Plug>                  * Install			PlugInstall
<Plug>                  * Update			PlugUpdate
<Plug>                  * Upgrade			PlugUpgrade
<Plug>                  * All				PlugUpgrade || PlugInstall || PlugUpdate

#remember key map
<Remember Key>          * nmap			call feedkeys(":Capture nmap\<CR>")
<Remember Key>          * imap			call feedkeys(":Capture imap\<CR>")
<Remember Key>          * vmap			call feedkeys(":Capture vmap\<CR>")

#Session
<Session>               * Save		call feedkeys(":SaveSession ")
<Session>               * Load		call feedkeys(":OpenSession ")
<Session>               * Delete	call feedkeys(":DeleteSession ")

#Vim
<Vim Settings>          * toggle relative Number	call feedkeys(":set relativenumber!\<CR>")
<Vim Settings>          * background=dark			set background=dark
<Vim Settings>          * background=light			set background=light
<Vim Settings>          * list						set list
<Vim Settings>          * nolist					set nolist

#Goyo
<Goyo>                  * GoyoNormalMode		call GoyoMode(11,66,65,65,245,0,"night-owl","dark")
<Goyo>                  * GoyoFocusWriteMode	call GoyoMode(12,80,65,65,255,1,"NeoSolarized","light")
<Goyo>                  * GoyoSubMonitorMode	call GoyoMode(12,80,60,60,220,1,"night-owl","dark")
<Goyo>                  * GoyoExit				call LeaveGoyoMode()
