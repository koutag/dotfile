/*レスポンシブ用にwidthにpaddingを含める*/
*{
        box-sizing: border-box;
}

/*aタグの下線を無効*/
a{
        text-decoration:none;
}

/*bodyの初期化*/
body{
        margin:0;
            padding:0;
}

/*font awesom用*/
.fas{
        padding: 0 5px 0 0;
}

/*floatクリア用*/
.clear{
        margin: 0;
            clear:both;
}
